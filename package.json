{
  "name": "lumbar",
  "version": "2.0.0",
  "description": "Supporting your backbone since 2011, Lumbar is a module build system that allows for generation of platform specific javascript modules.",
  "keywords": [
    "build",
    "module",
    "mobile",
    "backbone"
  ],
  "author": "Kevin Decker <kpdecker@gmail.com> (http://incaseofstairs.com)",
  "repository": {
    "type": "git",
    "url": "git://github.com/walmartlabs/lumbar.git"
  },
  "engines": {
    "node": ">=0.4.7"
  },
  "dependencies": {
    "async": "~0.2",
    "cheerio": "~0.11",
    "esprima": "~1.0",
    "growl": "~1.4",
    "handlebars": "~1.0",
    "nib": "~0.9",
    "optimist": "~0.4",
    "stylus": "~0.30",
    "stylus-images": "~1.0",
    "underscore": "~1.4",
    "coffee-script": "~1.6"
  },
  "devDependencies": {
    "mocha": "~1.9",
    "glob": "~3.2",
    "should": "*",
    "sinon": "*",
    "wrench": "~1.4",
    "grunt": "~0.4.1",
    "grunt-contrib-jshint": "~0.4.3",
    "grunt-mocha-cov": "0.0.5",
    "grunt-github-pages": "0.0.3"
  },
  "bin": {
    "lumbar": "bin/lumbar"
  },
  "main": "lib/lumbar",
  "scripts": {
    "blanket": {
      "pattern": "//^((?!/node_modules/)(?!/test/).)*$/",
      "data-cover-flags": {
        "branchTracking": true
      }
    },
    "test": "node_modules/.bin/mocha test/*.js test/plugins/*.js"
  },
  "optionalDependencies": {
    "source-map": "~0.1",
    "uglify-js": "~2.2"
  }
}
