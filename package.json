{
  "name": "lumbar",
  "version": "2.0.0-beta13",
  "description": "Supporting your backbone since 2011, Lumbar is a module build system that allows for generation of platform specific javascript modules.",
  "keywords": [
    "build",
    "module",
    "mobile",
    "backbone"
  ],
  "author": "Kevin Decker <kpdecker@gmail.com> (http://incaseofstairs.com)",
  "repository": {
    "type": "git",
    "url": "git://github.com/walmartlabs/lumbar.git"
  },
  "engines": {
    "node": ">=0.4.7"
  },
  "dependencies": {
    "async": "0.1.16",
    "cheerio": "~0.10",
    "esprima": "~1.0",
    "growl": "~1.4",
    "handlebars": "~1.0",
    "nib": "~0.9",
    "optimist": "~0.3",
    "stylus": "~0.30",
    "stylus-images": "~1.0",
    "underscore": "~1.4",
    "coffee-script": "1.3.3"
  },
  "devDependencies": {
    "mocha": "~1.7",
    "glob": "~3.0",
    "should": "*",
    "sinon": "*",
    "wrench": "~1.2"
  },
  "bin": {
    "lumbar": "bin/lumbar"
  },
  "main": "lib/lumbar",
  "scripts": {
    "test": "node_modules/.bin/mocha test/*.js test/plugins/*.js"
  },
  "optionalDependencies": {
    "source-map": "~0.1",
    "uglify-js": "~2.2"
  }
}
